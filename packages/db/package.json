{
  "name": "@acme/db",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts",
    "./schema": "./src/schema.ts",
    "./env": "./src/env.ts"
  },
  "license": "MIT",
  "scripts": {
    "clean": "git clean -xdf .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "migrate": "npm run with-env drizzle-kit migrate",
    "generate": "npm run with-env drizzle-kit generate",
    "studio": "npm run with-env drizzle-kit studio",
    "check": "npm run with-env drizzle-kit check:pg",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@t3-oss/env-core": "^0.10.1",
    "@t3-oss/env-nextjs": "^0.10.1",
    "drizzle-orm": "0.31.2",
    "drizzle-zod": "^0.5.1",
    "postgres": "^3.4.4",
    "zod": "^3.23.8",
    "@acme/eslint-config": "*",
    "@acme/prettier-config": "*",
    "@acme/tsconfig": "*",
    "@types/pg": "^8.11.6",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "0.22.6",
    "eslint": "^8.57.0",
    "prettier": "^3.3.1",
    "typescript": "^5.4.5"
  },
  "prettier": "@acme/prettier-config"
}
