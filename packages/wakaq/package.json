    "ai": "^3.4.9",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "drizzle-zod": "^0.5.1",
    "ms": "^2.1.3",
    "openai": "^4.49.1",
    "string-template": "^1.0.0",
    "superjson": "2.2.1",
    "ts-duration": "^1.1.0",
    "wakaq": "^2.2.1",
    "zod": "^3.23.8"
