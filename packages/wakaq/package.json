    "@types/string-template": "^1.0.6",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "prettier": "^3.3.1",
    "tsx": "^4.19.1",
    "typescript": "^5.4.5"
  },
  "prettier": "@acme/prettier-config"
